int main() {

    long a = 0;
    long b = a - 1;
    if (a>b) {
      printf("Signed longs work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with signed long.\n");
    }

    ulong c = 0;
    ulong d = c - 1;
    if (c<d) {
      printf("Unsigned longs work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with unsigned long.\n");
    }

    long e = 0;
    long f = e - 1;
    if (e>f) {
      printf("Signed ints work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with signed int.\n");
    }

    ulong g = 0;
    ulong h = g - 1;
    if (g<h) {
      printf("Unsigned ints work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with unsigned int.\n");
    }

	return 0;
}
