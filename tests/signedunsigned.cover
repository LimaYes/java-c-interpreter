int main() {

    long a = 0;
    long b = a - 1;
    if (a>b) {
      printf("Signed longs work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with signed long.\n");
    }

    ulong c = 0;
    ulong d = c - 1;
    if (c<d) {
      printf("Unsigned longs work as expected.\n");
    }
    else {
      printf("Oops, something went wrong with unsigned long.\n");
    }


	return 0;
}
